port={{confluent_replicator_jolokia_port}}
host=0.0.0.0
{% if confluent_replicator_jolokia_ssl_enabled|bool %}
keystore={{confluent_replicator_keystore_path}}
keystorePassword={{confluent_replicator_keystore_storepass}}
protocol=https
sslProtocol=TLSv1.2
{% endif %}
{% if confluent_replicator_jolokia_auth_mode == 'basic' %}
authMode=basic
user={{confluent_replicator_jolokia_user}}
password={{confluent_replicator_jolokia_password}}
{% endif %}
