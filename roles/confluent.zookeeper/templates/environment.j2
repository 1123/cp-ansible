[Service]
{% for key, value in zookeeper.environment.items() %}
Environment="{{key}}={{value}}"
{% endfor %}
{% if security_mode in ["kerberos", "kerberos_ssl", "kerberos_ssl_customcerts"] %}
{% for key, value in zookeeper.kerberosEnvironment.items() %}
Environment="{{key}}={{value}}"
{% endfor %}
{% endif %}
