# Maintained by Ansible
{% for key, value in zookeeper.properties.items() %}
{{key}}={{value}}
{% endfor %}
{% for host in groups['zookeeper'] %}
{% if hostvars[host]['zookeeper_id'] is defined %}
{% set id = hostvars[host]['zookeeper_id'] %}
{% else %}
{% set id = loop.index %}
{% endif %}
server.{{ id }}={{ host }}:2888:3888
{% endfor %}
